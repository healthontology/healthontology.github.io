##### VOCABS

hero:Vocabulary rdf:type owl:Class ;
               rdfs:label "Clincial Vocabulary" .

vocab:AdverseReactionVerification rdf:type owl:Class ;
       rdfs:subClassOf hero:Vocabulary.

vocab:AdverseReactionStatus rdf:type owl:Class ;
       rdfs:subClassOf hero:Vocabulary.
  
vocab:DiagnosisStatus rdf:type owl:Class ;
       rdfs:subClassOf hero:Vocabulary.

vocab:SmokingStatus rdf:type owl:Class ;
       rdfs:subClassOf hero:Vocabulary.

vocab:SmokingVariety rdf:type owl:Class ;
       rdfs:subClassOf hero:Vocabulary.


### COMMON  

hero:ClinicalInteraction rdf:type owl:Class ;
               rdfs:label "Clincial Interaction" ;
               rdfs:comment "The parent to all clinical interaction" .

hero:ClinicalArtefact rdf:type owl:Class ;
               rdfs:label "Clincial Artefact" ;
               rdfs:comment "Supporting clinical artefacts" .

####### ADVERSE REACTION

hero:AdverseReaction rdf:type owl:Class ;
               rdfs:subClassOf hero:ClinicalInteraction ;
               rdfs:label "Adverse Reaction" ;
               rdfs:comment "A summary or overview of a clinical assessment that identifies the potential for a harmful or undesirable physiological reaction unique to an individual and associated with exposure to a specific substance" .

hero:Substance rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
               rdfs:label "Substance" ;
               rdfs:comment "Substance TYPE defined by CLINICAL TERMINOLOGY" .

hero:hasAdverseReactionSubstance rdf:type owl:ObjectProperty ;
                    rdfs:label "Has a Adverse Reaction Substance" ;
                    rdfs:comment "Identification of a specific substance, compounded product, combinaWon therapy product, or class of substances that is considered to have a causal link to the adverse reaction" ;
                    rdfs:domain hero:AdverseReaction ;
                    rdfs:range hero:Substance .

hero:hasAdverseReactionStatus rdf:type owl:ObjectProperty ;
                    rdfs:label "Has a Adverse Reaction Status" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:AdverseReaction ;
                    rdfs:range vocab:AdverseReactionStatus .

hero:hasAdverseReactionVerificationStatus rdf:type owl:ObjectProperty ;
                    rdfs:label "Has a Adverse Reaction Verification Status" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:AdverseReaction ;
                    rdfs:range vocab:AdverseReactionVerification .

hero:AdverseReactionEvent rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
                   rdfs:label "Adverse Reaction Event" .

hero:hasAdverseReactionEvent rdf:type owl:ObjectProperty ;
                    rdfs:label "Has an Adverse Reaction Event" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:AdverseReaction ;
                    rdfs:range hero:AdverseReactionEvent .

hero:Manifestation rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
                   rdfs:label "Manifestation" ;
                   rdfs:comment "Manifestation TYPE defined by CLINICAL TERMINOLOGY" .

hero:hasAdverseReactionManifestation rdf:type owl:ObjectProperty ;
                    rdfs:label "Has a Adverse Reaction Manifestation" ;
                    rdfs:comment "Symptom or sign observed or associated with the reaction" ;
                    rdfs:domain hero:AdverseReactionEvent ;
                    rdfs:range hero:Manifestation .

hero:dateOfAdverseReactionEventOnsetDate rdf:type owl:DatatypeProperty ;
                    rdfs:label "Date of Adverse Reaction Event Onset" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:AdverseReactionEvent ;
                    rdfs:range xsd:date .     

hero:commentOnAdverseReactionComment rdf:type owl:DatatypeProperty ;
                    rdfs:label "Comment on Adverse Reaction" ;
                    rdfs:comment "Additiononal narrative about the adverse reaction risk" ;
                    rdfs:domain hero:AdverseReaction ;
                    rdfs:range rdf:langString .              



### CLINCAL DIAGNOSIS


hero:ClinicalDiagnosis rdf:type owl:Class ;
               rdfs:subClassOf hero:ClinicalInteraction ;
               rdfs:label "Clinical Diagnosis/Problem" ;
               rdfs:comment "" .

hero:Diagnosis rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
               rdfs:label "Diagnosis/Problem" ;
               rdfs:comment "Diagnosis/Problem TYPE defined by CLINICAL TERMINOLOGY" .

hero:hasDiagnosis rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Diagnosis" ;
                    rdfs:comment "Identification of the Diagnosis/Problem" ;
                    rdfs:domain hero:ClinicalDiagnosis ;
                    rdfs:range hero:Diagnosis .

hero:BodySite rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
               rdfs:label "Body Site/Laterality" ;
                rdfs:comment "Bodysite TYPE defined by CLINICAL TERMINOLOGY" .


hero:hasDiagnosisBodySite rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Procedure Body Site" ;
                    rdfs:comment "The anatomical location or body structure where the procedure was performed" ;
                    rdfs:domain hero:Diagnosis ;
                    rdfs:range hero:BodySite ;
                    owl:equivalentProperty snomed:363698007 .


hero:hasDiagnosisStatus rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Diagnosis Status" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:ClinicalDiagnosis ;
                    rdfs:range vocab:DiagnosisStatus .


hero:dateDiagnosisRecognised rdf:type owl:DatatypeProperty ;
                    rdfs:label "Date Diagnosis Recognised" ;
                    rdfs:comment "" ;
                    rdfs:domain hero:ClinicalDiagnosis ;
                    rdfs:range xsd:dateTime .         

hero:commentOnDiagnosis rdf:type owl:DatatypeProperty ;
                    rdfs:label "Comment on Diagnosis" ;
                    rdfs:comment "Additional narrative about the Diagnosis" ;
                    rdfs:domain hero:ClinicalDiagnosis ;
                    rdfs:range rdf:PlainLiteral .   


### CLINICAL PROCEDURE

hero:ClinicalProcedure rdf:type owl:Class ;
               rdfs:subClassOf hero:ClinicalInteraction ;
               rdfs:label "Clinical Procedure" ;
               rdfs:comment "A clinical procedure is an intentional intervention to diagnose, treat or manage a health condition, often involving invasive or potentialy harmful techniques requiring skin or mucosal penetration or tissue manipulation" .

hero:Procedure rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
               rdfs:label "Procedure" ;
                rdfs:comment "Procedure TYPE defined by CLINICAL TERMINOLOGY" .

hero:hasProcedure rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Procedure" ;
                    rdfs:comment "Identification of the Procedure" ;
                    rdfs:domain hero:ClinicalProcedure ;
                    rdfs:range hero:Procedure .

hero:hasProcedureBodySite rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Procedure Body Site" ;
                    rdfs:comment "The anatomical location or body structure where the procedure was performed" ;
                    rdfs:domain hero:Procedure ;
                    rdfs:range hero:BodySite ;
                    owl:equivalentProperty snomed:405813007 .

hero:Indication rdf:type owl:Class ;
                  rdfs:subClassOf hero:ClinicalArtefact ;
               rdfs:label "Clincial Indication" ;
               rdfs:comment "Clincial Indication TYPE defined by CLINICAL TERMINOLOGY" .

hero:hasClinicalIndication rdf:type owl:ObjectProperty ;
                    rdfs:label "Has Clinical Indication" ;
                    rdfs:comment "The clinical symptom, sign or diagnosis that necessitates the procedure" ;
                    rdfs:domain hero:ClinicalProcedure ;
                    rdfs:range hero:Indication .


hero:dateProcedureFinalised rdf:type owl:DatatypeProperty ;
                    rdfs:label "Date Procedure Finalised" ;
                    rdfs:comment "Date when the Procedure all completed" ;
                    rdfs:domain hero:ClinicalProcedure ;
                    rdfs:range xsd:date .         

hero:commentOnProcedure rdf:type owl:DatatypeProperty ;
                    rdfs:label "Comment on Procedure" ;
                    rdfs:comment "Additional narrative about the Procedure" ;
                    rdfs:domain hero:ClinicalProcedure ;
                    rdfs:range rdf:PlainLiteral .   

##### VACCINATION 


hero:VaccinationAdminstration rdf:type owl:Class ;
